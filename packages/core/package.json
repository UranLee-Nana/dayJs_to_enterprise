{
  "name": "@dayjs-business/core",
  "version": "1.0.0",
  "description": "Core layer for dayjs-business - factory functions, plugin registry, and type definitions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "dayjs": "^1.11.10"
  },
  "peerDependencies": {
    "dayjs": ">=1.11.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "keywords": ["dayjs", "date", "time", "business", "enterprise", "typescript"],
  "license": "MIT",
  "author": "uranlxy <uranlxy235@gmail.com>"
}
